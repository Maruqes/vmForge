frontend ssh
    maxconn 10000
    bind *:mainPort ssl crt haproxyFiles/ssh.pem
    timeout client 2147483647ms
    mode tcp
    #ssl_fc_sni
    tcp-request content set-var(sess.sni) ssl_fc_sni
    use_backend %[ssl_fc_sni]

# ports frontend for HTTP
frontend http
    bind *:8081
    mode http
    timeout client 50000ms